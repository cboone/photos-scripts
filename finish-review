#!/usr/bin/env bash

function main {
  echo 'finish-review: Newly imported photos have been reviewed (rejected and selected)'

  trash-rejected-photos
  move-and-import-selected-photos
  echo 'Done importing selects into Lightroom?'
  read -p 'Press any key to move all remaining reviewed photos or control-c to cancel... ' -n1 -r -s
  echo ''
  move-reviewed-photos
}

main
